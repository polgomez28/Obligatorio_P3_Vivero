@model Dominio.TipoPlanta

@{
    ViewData["Title"] = "Busqueda";
}

<h1>Busqueda</h1>

<h4>Tipo Planta</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Search">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TipoNombre" class="control-label"></label>
                <input asp-for="TipoNombre" class="form-control" />
                <span asp-validation-for="TipoNombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Buscar" placeholder="Nombre Tipo" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>